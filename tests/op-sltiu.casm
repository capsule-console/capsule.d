; Test sltiu (Set if less than immediate unsigned)
; Store 1 to rd if rs1 < imm, otherwise store 0 to rd

.const meta.exit_ok, 0x0001
.const stdio.init, 0x3000
.const stdio.put_byte, 0x3002

.text
.entry
    ; Initialize
    ecalli Z, Z, stdio.init
    ; 1
    li X, 0
    sltiu X, X, 1
    addi X, X, '0'
    ecalli Z, X, stdio.put_byte
    ; 1
    li X, 0
    sltiu X, X, 0xffff
    addi X, X, '0'
    ecalli Z, X, stdio.put_byte
    ; 1
    li X, 1234
    sltiu X, X, 5678
    addi X, X, '0'
    ecalli Z, X, stdio.put_byte
    ; 0
    li X, 1
    sltiu X, X, 0
    addi X, X, '0'
    ecalli Z, X, stdio.put_byte
    ; 0
    li X, 0xffffffff
    sltiu X, X, 0
    addi X, X, '0'
    ecalli Z, X, stdio.put_byte
    ; 0
    li X, 6000
    sltiu X, X, 1200
    addi X, X, '0'
    ecalli Z, X, stdio.put_byte
    ; 0
    li X, 512
    sltiu X, X, 512
    addi X, X, '0'
    ecalli Z, X, stdio.put_byte
    ; 0
    li X, 0x4000
    sltiu X, X, 0x4000
    addi X, X, '0'
    ecalli Z, X, stdio.put_byte
    ; 1
    li X, 0x8000
    sltiu X, X, 0x8000
    addi X, X, '0'
    ecalli Z, X, stdio.put_byte
    ; All done
    ecalli Z, Z, meta.exit_ok
