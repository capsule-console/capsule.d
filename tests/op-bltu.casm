; Test bltu (Branch if less than unsigned)
; Add imm to PC if rs1 < rs2

.const meta.exit_ok, 0x0001
.const stdio.init, 0x3000
.const stdio.put_byte, 0x3002

.text
.entry
    ; Initialize
    ecalli Z, Z, stdio.init
    li A, 'N'
    li B, 'Y'
    ; Y
    li X, 0
    li Y, 1
    bltu X, Y, 1f
    ecalli Z, A, stdio.put_byte j 0f
1:  ecalli Z, B, stdio.put_byte
0:
    ; Y
    li X, 0
    li Y, 0xffffffff
    bltu X, Y, 1f
    ecalli Z, A, stdio.put_byte j 0f
1:  ecalli Z, B, stdio.put_byte
0:
    ; Y
    li X, 1234
    li Y, 5678
    bltu X, Y, 1f
    ecalli Z, A, stdio.put_byte j 0f
1:  ecalli Z, B, stdio.put_byte
0:
    ; N
    li X, 1
    li Y, 0
    bltu X, Y, 1f
    ecalli Z, A, stdio.put_byte j 0f
1:  ecalli Z, B, stdio.put_byte
0:
    ; N
    li X, 0xffffffff
    li Y, 0
    bltu X, Y, 1f
    ecalli Z, A, stdio.put_byte j 0f
1:  ecalli Z, B, stdio.put_byte
0:
    ; N
    li X, 6000
    li Y, 1200
    bltu X, Y, 1f
    ecalli Z, A, stdio.put_byte j 0f
1:  ecalli Z, B, stdio.put_byte
0:
    ; N
    li X, 512
    li Y, 512
    bltu X, Y, 1f
    ecalli Z, A, stdio.put_byte j 0f
1:  ecalli Z, B, stdio.put_byte
0:
    ; N
    li X, 0x80000000
    li Y, 0x80000000
    bltu X, Y, 1f
    ecalli Z, A, stdio.put_byte j 0f
1:  ecalli Z, B, stdio.put_byte
0:
    ; All done
    ecalli Z, Z, meta.exit_ok
