; Test bge (Branch if greater than or equal signed)
; Add imm to PC if rs1 >= rs2

.const meta.exit_ok, 0x0001
.const stdio.init, 0x3000
.const stdio.put_byte, 0x3002

.text
.entry
    ; Initialize
    ecalli Z, Z, stdio.init
    li A, 'N'
    li B, 'Y'
    ; Positive >= Positive (N)
    li X, 99
    li Y, 100
    bge X, Y, 1f
    ecalli Z, A, stdio.put_byte j 0f
1:  ecalli Z, B, stdio.put_byte
0:
    ; Positive !>= Positive (Y)
    li X, 60
    li Y, 15
    bge X, Y, 1f
    ecalli Z, A, stdio.put_byte j 0f
1:  ecalli Z, B, stdio.put_byte
0:
    ; Positive == Positive (Y)
    li X, 3_500
    li Y, 3_500
    bge X, Y, 1f
    ecalli Z, A, stdio.put_byte j 0f
1:  ecalli Z, B, stdio.put_byte
0:
    ; Negative >= Negative (N)
    li X, -90
    li Y, -10
    bge X, Y, 1f
    ecalli Z, A, stdio.put_byte j 0f
1:  ecalli Z, B, stdio.put_byte
0:
    ; Negative !>= Negative (Y)
    li X, -75
    li Y, -80
    bge X, Y, 1f
    ecalli Z, A, stdio.put_byte j 0f
1:  ecalli Z, B, stdio.put_byte
0:
    ; Negative == Negative (Y)
    li X, -1
    li Y, -1
    bge X, Y, 1f
    ecalli Z, A, stdio.put_byte j 0f
1:  ecalli Z, B, stdio.put_byte
0:
    ; Negative >= Positive (N)
    li X, -50
    li Y, +50
    bge X, Y, 1f
    ecalli Z, A, stdio.put_byte j 0f
1:  ecalli Z, B, stdio.put_byte
0:
    ; Positive !>= Negative (Y)
    li X, +25
    li Y, -16
    bge X, Y, 1f
    ecalli Z, A, stdio.put_byte j 0f
1:  ecalli Z, B, stdio.put_byte
0:
    ; All done
    ecalli Z, Z, meta.exit_ok
